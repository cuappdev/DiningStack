use_frameworks!
source 'https://github.com/CocoaPods/Specs.git'

def shared_pods
    pod 'Alamofire', '~> 3.1.5'
    pod 'SwiftyJSON', '~> 2.3.2'
end

target :'DiningStack' do
    platform:ios, '8.0'
    shared_pods
end

target :'DiningStack WatchKit' do
    platform :watchos, '2.0'
    shared_pods
end
